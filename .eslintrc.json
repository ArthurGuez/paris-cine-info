{
	"extends": ["airbnb", "airbnb/hooks", "next/core-web-vitals", "prettier"],
	"rules": {
		"import/extensions": [
			"error",
			"ignorePackages",
			{
				"js": "never",
				"jsx": "never",
				"ts": "never",
				"tsx": "never"
			}
		],
		"react/jsx-filename-extension": [2, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }],
		"react/jsx-props-no-spreading": "off"
	}
}
